class Solution {
    public int heightChecker(int[] heights) {
        int n = heights.length;
        int rt = 0;
        int[] nh = new int[n];
        for(int i=0; i<n; i++){
            nh[i]=heights[i];
        }

        Arrays.sort(nh);

        for(int i=0; i<n; i++){
            if(nh[i]!=heights[i]){
                rt++;
            }
        }

        return rt;
    }
}
