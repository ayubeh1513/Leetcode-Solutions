class Solution {
    public int missingNumber(int[] nums) {
        ArrayList<Integer> a = new ArrayList<>();
        int m = nums[0];
        for(int i : nums){
            if(i>m) m=i;
        }
        for (int i : nums) {
            a.add(i);
        }
        for(int i=0; i<a.size(); i++){
            if(!a.contains(i)) return i;
        }
    return m+1;
    }
}
