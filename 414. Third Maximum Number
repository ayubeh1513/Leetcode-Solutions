class Solution {
    public int thirdMax(int[] nums) {
        Arrays.sort(nums);
        int n = nums.length;
        ArrayList<Integer> arr = new ArrayList<>();
        arr.add(nums[0]);
        for(int i=1; i<nums.length; i++){
            if(nums[i]!=nums[i-1]){
                arr.add(nums[i]);
            }
        }

        if(arr.size() == 1){
            return arr.get(arr.size()-1);
        } else if (arr.size() == 2){
            return arr.get(arr.size()-1);
        } else {
            return arr.get(arr.size()-3);
        }
    }
}
